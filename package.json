{
  "name": "@inrupt/solid-client-access-grants",
  "description": "A library for managing access grants in Solid",
  "version": "0.3.2",
  "license": "MIT",
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "build-api-docs": "rm -rf docs/api/source/api/ && typedoc",
    "build-docs-preview-site": "npm run build-api-docs; cd docs/api; rm source/api/index.md; make html; cd ../; rm -r dist || true; mkdir -p dist/api; cp -r api/build/html/. dist/;",
    "lint": "eslint --max-warnings 0 src",
    "test": "jest src",
    "e2e-test-node": "jest --config=jest.e2e.config.js e2e/node",
    "e2e-test-browser": "testcafe",
    "list-licenses": "license-checker --production --csv --out LICENSE_DEPENDENCIES_ALL",
    "check-licenses": "license-checker --production --failOn \"AGPL-1.0-only; AGPL-1.0-or-later; AGPL-3.0-only; AGPL-3.0-or-later; Beerware; CC-BY-NC-1.0; CC-BY-NC-2.0; CC-BY-NC-2.5; CC-BY-NC-3.0; CC-BY-NC-4.0; CC-BY-NC-ND-1.0; CC-BY-NC-ND-2.0; CC-BY-NC-ND-2.5; CC-BY-NC-ND-3.0; CC-BY-NC-ND-4.0; CC-BY-NC-SA-1.0; CC-BY-NC-SA-2.0; CC-BY-NC-SA-2.5; CC-BY-NC-SA-3.0; CC-BY-NC-SA-4.0; CPAL-1.0; EUPL-1.0; EUPL-1.1; EUPL-1.1;  GPL-1.0-only; GPL-1.0-or-later; GPL-2.0-only;  GPL-2.0-or-later; GPL-3.0; GPL-3.0-only; GPL-3.0-or-later; SISSL;  SISSL-1.2; WTFPL\"",
    "prepublishOnly": "npm run build",
    "prepare": "husky install"
  },
  "keywords": [
    "rdf",
    "solid",
    "linked data",
    "turtle"
  ],
  "homepage": "https://docs.inrupt.com/client-libraries/solid-client-access-grants-js/",
  "bugs": "https://github.com/inrupt/solid-client-access-grants-js/issues",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./discover": "./dist/discover/index.mjs",
    "./manage": "./dist/manage/index.mjs",
    "./request": "./dist/request/index.mjs",
    "./verify": "./dist/verify/index.mjs",
    "./resource": "./dist/resource/index.mjs",
    "./fetch": "./dist/resource/index.mjs"
  },
  "typesVersions": {
    "*": {
      "discover": [
        "dist/discover/index.d.ts"
      ],
      "manage": [
        "dist/manage/index.d.ts"
      ],
      "request": [
        "dist/request/index.d.ts"
      ],
      "verify": [
        "dist/verify/index.d.ts"
      ],
      "resource": [
        "dist/resource/index.d.ts"
      ],
      "fetch": [
        "dist/fetch/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "src",
    "umd"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/inrupt/solid-client-access-grants-js.git"
  },
  "devDependencies": {
    "@inrupt/eslint-config-base": "^0.2.0-alpha.1",
    "@inrupt/eslint-config-lib": "^0.2.0-alpha.1",
    "@inrupt/solid-client-authn-browser": "^1.8.1",
    "@inrupt/solid-client-authn-node": "^1.11.2",
    "@skypack/package-check": "^0.2.2",
    "@testing-library/testcafe": "^4.4.0",
    "@types/auth-header": "^1.0.2",
    "@types/dotenv-flow": "^3.1.0",
    "@types/jest": "^26.0.21",
    "@types/parse-link-header": "^1.0.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "dotenv-flow": "^3.2.0",
    "eslint": "^7.29.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-header": "^3.1.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-license-header": "^0.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "fast-check": "^2.17.0",
    "husky": "^6.0.0",
    "jest": "^27.0.6",
    "license-checker": "^25.0.1",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.2",
    "rollup": "^2.52.4",
    "rollup-plugin-typescript2": "^0.30.0",
    "testcafe": "^1.14.2",
    "ts-jest": "^27.0.3",
    "typedoc": "^0.21.2",
    "typedoc-plugin-markdown": "^3.10.2",
    "typescript": "^4.3.4"
  },
  "dependencies": {
    "@inrupt/solid-client": "^1.14.0",
    "@inrupt/solid-client-authn-core": "^1.11.2",
    "@inrupt/solid-client-vc": "^0.2.2",
    "auth-header": "^1.0.0",
    "cross-fetch": "^3.1.4",
    "events": "^3.3.0",
    "form-urlencoded": "^6.0.5",
    "parse-link-header": "^1.0.1",
    "rdf-namespaces": "^1.9.2"
  },
  "lint-staged": {
    "src/*.{js,jsx,ts,tsx}": "eslint --max-warnings 0 --cache --fix",
    "*.{ts,tsx,js,jsx,css,md,mdx}": "prettier --write"
  },
  "resolutions": {
    "glob-parent": "^5.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}
